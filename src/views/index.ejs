                                                    <img class="aspect__img"
                                                         src="<%= value.images.length ? value.images[0] : '/images/product/electronic/product2.jpg' %>"
                                                         alt=""></a>
                                                <div class="product-o__action-wrap">
                                                    <ul class="product-o__action-list">
                                                        <li>

                                                            <a data-modal="modal" data-modal-id="#quick-look"
                                                               data-tooltip="tooltip" data-placement="top"
                                                               title="Quick View"
                                                               onclick="openModalQuickLook(<%= JSON.stringify(value) %>)"
                                                            ><i class="fas fa-search-plus"></i></a>
                                                        </li>
                                                        <li>

                                                            <a data-modal="modal" data-modal-id="#add-to-cart"
                                                               href="/cart/add?id=<%= value._id %>"
                                                               data-tooltip="tooltip" data-placement="top"
                                                               title="Add to Cart"><i
                                                                        class="fas fa-plus-circle"></i></a></li>
                                                    </ul>
                                                </div>
                                            </div>

                                            <span class="product-o__category">

                                                    <a href="shop-side-version-2.html"><%= value.category %></a></span>

                                            <span class="product-o__name">

                                                    <a href="/product/detail/?_id=<%= value._id %>"><%= value.name %></a></span>
